<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:left="3dp"
        android:right="3dp"
        android:top="20dp">
        <shape>
            <corners android:radius="30dp" />
            <solid android:color="#FFD233" />
            <stroke
                android:width="5dp"
                android:color="#FFD233" />
        </shape>
    </item>
    <item android:bottom="5dp">

        <shape android:shape="rectangle">
            <corners android:radius="30dp" />
            <solid android:color="#fff" />
            <stroke
                android:width="1dp"
                android:color="#B7A8A8A8" />
        </shape>
    </item>


</layer-list>